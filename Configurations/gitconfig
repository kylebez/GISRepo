[diff "astextplain"]
	textconv = astextplain
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[http]
	sslBackend = openssl
	sslCAInfo = C:/Users/t98007/AppData/Local/Programs/Git/mingw64/ssl/certs/ca-bundle.crt
[core]
	autocrlf = true
	fscache = true
	symlinks = true
	editor = \"C:\\\\Program Files\\\\Notepad++\\\\notepad++.exe\" -multiInst -notabbar -nosession -noPlugin
	useBuiltinFSMonitor = true
[pull]
	rebase = false
[credential]
	helper = manager-core
[credential "https://dev.azure.com"]
	useHttpPath = true
[init]
	defaultBranch = master
[color "status"]
  added = green bold
  changed = red bold strike
  untracked = cyan
  branch = yellow black bold ul
[alias]
  forgotten = branch --no-merged master
  force-commit = "!f() { git commit --no-verify -m \"$1\"; }; f"
  sas = "!f() { git stash push -m \"working\" -u && git switch $1; }; f" 
  sap = "!f() { git switch $1 && git stash pop; }; f"
  update-master = "!f() { cb=$(git branch --show-current); git sas master && git pull && git sap $cb; }; f"